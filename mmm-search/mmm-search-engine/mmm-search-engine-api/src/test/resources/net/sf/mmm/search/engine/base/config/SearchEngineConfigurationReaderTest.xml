<search>
  <!-- configuration of sources -->
  <sources>
    <source id="" title="All"/>
    <source id="twiki" title="Wiki" url-prefix="http://foo.bar/twiki/bin/view/"/>
    <source id="svn" title="Subversion" url-prefix="http://foo.bar/svn/trunk/"/>
  </sources>
  <!-- configuration of file-types for view (webapp) -->
  <entry-types>
    <type id="" title="All"/>
    <type id="default" icon="file.png"/>
    <type id="pdf" title="PDF" icon="pdf.png"/>
    <type id="doc" title="Word" icon="word.png"/>
    <type id="txt" title="Text" icon="text.png"/>
    <type id="xml" title="XML" icon="xml.png"/>
    <type id="html" title="HTML" icon="firefox.png"/>
    <type id="htm" icon="ie.png"/>
  </entry-types>
  <search-properties>
    <properties>
      <property key="foo">bar</property>
    </properties>
  </search-properties>
</search>