<?xml version="1.0" encoding="UTF-8"?>
  <!-- $Id$ -->
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
  xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

  <util:map id="ContentParserAliasMap">
    <!-- alias mapping, overridden if specific parser is registered -->
    <entry key="htm" value="html"/>
    <entry key="php" value="html"/>
    <entry key="jsp" value="html"/>
    <entry key="jinc" value="text-with-markup"/>
    <entry key="asc" value="txt"/>
    <entry key="apt" value="txt"/>
    <entry key="csv" value="txt"/>
    <entry key="sql" value="txt"/>
    <entry key="bat" value="txt"/>
    <entry key="sh" value="txt"/>
    <entry key="java" value="txt"/>
    <entry key="c" value="txt"/>
    <entry key="h" value="txt"/>
    <entry key="cpp" value="txt"/>
    <entry key="c++" value="txt"/>
    <entry key="cs" value="txt"/>
    <entry key="net" value="txt"/>
    <entry key="pl" value="txt"/>
    <entry key="py" value="txt"/>
    <entry key="js" value="txt"/>
    <!-- MS-Office Templates -->
    <entry key="dot" value="doc"/>
  </util:map>
</beans>