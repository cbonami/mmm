 --%><%
if (currentPage > 1) {
  %>
<a href="javascript:showPage(0)"><img src="images/arrows/arrow-first-on.gif" alt="first"/></a>
<a href="javascript:showPage(<%= currentPage - 2 %>)"><img src="images/arrows/arrow-previous-on.gif" alt="previous"/></a>
  <%    
} else {
  %>
<img src="images/arrows/arrow-first-off.gif" alt="first"/>
<img src="images/arrows/arrow-previous-off.gif" alt="previous"/>
  <%
}

for (int pageIndex = pagingStart; pageIndex <= pagingEnd; pageIndex++) {
  if ((pageIndex  + 1)== currentPage) {
    %><%= currentPage%> <%
  } else {
    %><a href="javascript:showPage(<%= pageIndex %>)"><%= pageIndex + 1%></a> <%
  }
}
if (currentPage < maxPage) {
  %>
<a href="javascript:showPage(<%= currentPage %>)"><img src="images/arrows/arrow-next-on.gif" alt="next"/></a>
<a href="javascript:showPage(<%= maxPage - 1 %>)"><img src="images/arrows/arrow-last-on.gif" alt="last"/></a>
  <%  
} else {
  %>
<img src="images/arrows/arrow-next-off.gif" alt="next"/>
<img src="images/arrows/arrow-last-off.gif" alt="last"/>
  <%
}
%>
